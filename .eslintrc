{
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "env": {
        "es6": true,
        "node": true
    },
    "rules": {
        "consistent-return": 2,
        "indent": [
            1,
            4
        ],
        "no-else-return": 1,
        "space-unary-ops": 2,
        "no-console": 1
    }
}